-- Scripts:

local function JOCGIV_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0)do
		script.Parent.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)
	
		counter = counter + 0.01
	end
end
coroutine.wrap(JOCGIV_fake_script)()
local function DRGF_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(DRGF_fake_script)()
local function TGOI_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		if game.Players.LocalPlayer.PlayerGui.MagicWare.Premium.Drag.Disabled == false and game.Players.LocalPlayer.PlayerGui.settings.Settings.Drag.Disabled == false and game.Players.LocalPlayer.PlayerGui.MagicWare.Premium2.Drag.Disabled == false and game.Players.LocalPlayer.PlayerGui.MagicWare.Premium3.Drag.Disabled == false and game.Players.LocalPlayer.PlayerGui.MagicWare.Premium4.Drag.Disabled == false then
			script.Parent.Text = "Smooth Drag: Off"
			script.Parent.Parent.Parent:Destroy()
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium.Drag.Disabled = true
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium2.Drag.Disabled = true
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium3.Drag.Disabled = true
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium4.Drag.Disabled = true
			game.Players.LocalPlayer.PlayerGui.settings.Settings.Drag.Disabled = true
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium.NormalDrag.Disabled = false
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium2.NormalDrag.Disabled = false
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium3.NormalDrag.Disabled = false
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium4.NormalDrag.Disabled = false
			game.Players.LocalPlayer.PlayerGui.settings.Settings.NormalDrag.Disabled = false
		else
			script.Parent.Text = "Smooth Drag: On"
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium.Drag.Disabled = false
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium2.Drag.Disabled = false
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium3.Drag.Disabled = false
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium4.Drag.Disabled = false
			game.Players.LocalPlayer.PlayerGui.settings.Settings.Drag.Disabled = false
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium.NormalDrag.Disabled = true
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium2.NormalDrag.Disabled = true
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium3.NormalDrag.Disabled = true
			game.Players.LocalPlayer.PlayerGui.MagicWare.Premium4.NormalDrag.Disabled = true
			game.Players.LocalPlayer.PlayerGui.settings.Settings.NormalDrag.Disabled = true
		end
	end)
end
coroutine.wrap(TGOI_fake_script)()
local function XNVCA_fake_script() -- Settings.Drag 
	local script = Instance.new('LocalScript', Settings)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
		dragToggle = nil
		local dragSpeed = 0.50
		dragInput = nil
		dragStart = nil
		local dragPos = nil
		function updateInput(input)
			local Delta = input.Position - dragStart
			local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.30), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(XNVCA_fake_script)()
-- Settings.NormalDrag is disabled.
